{
    "name": "Redmine App",
    "description": "Redmine App",
    "version": "1.0.1",
    "manifest_version": 2,
    "update_url": "http://clients2.google.com/service/update2/crx",
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    },
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "permissions": [
        "storage",
        "<all_urls>"
    ]
}
